#include <bits/stdc++.h>
using namespace std;

bool isNumber(char c) {
	if (c == '0' || c == '1' || c == '2' || c == '3' || c == '4' || c == '5'
			|| c == '6' || c == '7' || c == '8' || c == '9')
		return true;
	else
		return false;
}

int main() {
	string S;
	cin >> S;
	if (isNumber(S[0]) && isNumber(S[1]) && isNumber(S[2]))
		cout << stoi(S) * 2 << endl;
	else
		cout << "error" << endl;
	return 0;
}
