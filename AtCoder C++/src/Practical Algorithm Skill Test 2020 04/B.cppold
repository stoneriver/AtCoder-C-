#include <bits/stdc++.h>
using namespace std;

int main() {
	int a = 0, b = 0, c = 0;
	string S;
	cin >> S;
	for (int i = 0; i < S.size(); i++) {
		if (S[i] == 'a')
			a++;
		else if (S[i] == 'b')
			b++;
		else if (S[i] == 'c')
			c++;
	}
	if (a > b && a > c)
		cout << "a" << endl;
	else if (b > c)
		cout << "b" << endl;
	else
		cout << "c" << endl;
	return 0;
}
